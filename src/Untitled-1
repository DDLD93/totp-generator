
db.onSnapshot((querySnapshot) => {
      const items = [];
      querySnapshot.forEach((doc) => {
        items.push(doc.data());
      });
      setUser(items);
      console.log(user)
    }); 


    setTimeout(() => {
   for (let index = 0; index < user.length; index++) {
     const element = user[index].code;
     var codes=  getcode(element);
     setUser({...user,  user: {...user[index], 
      time: codes
      }})
      console.log(user);
     
}
  //console.log(user[0]);
 },1000)